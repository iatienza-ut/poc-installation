{
    "manifest_version": 3,
    "name": "Installation check detection V3",
    "version": "1.0",
    "icons": {
        "48": "icons/message-48.png"
    },
    "action": {
        "default_title": "__MSG_extension_title__",
        "default_icon": {
            "48": "icons/message-48.png"
        }
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content-script.js"],
            "run_at": "document_start",
            "all_frames": false
        }
    ],
    "permissions": ["storage", "tabs"],
    "background": {
        "service_worker": "service-worker.js"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "default_locale": "en"
}
