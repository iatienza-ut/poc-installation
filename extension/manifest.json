{
    "manifest_version": 3,
    "name": "Installation check detection V3",
    "version": "1.0",
    "icons": {
        "48": "icons/message-48.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content-script.js"],
            "run_at": "document_start"
        }
    ],
    "permissions": ["storage", "tabs"],
    "background": {
        "service_worker": "service-worker.js"
    },
    "host_permissions": [
        "<all_urls>"
    ]
}
