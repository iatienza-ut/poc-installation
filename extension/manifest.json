{
    "manifest_version": 2,
    "name": "Installation check detection",
    "version": "1.0",
    "icons": {
        "48": "icons/message-48.png"
    },
    "content_scripts": [
        {
            "matches": ["https://iatienza-uz.github.io/poc-installation/*"],
            "js": ["content-script.js"],
            "run_at": "document_start"
        }
    ],
    "permissions": ["storage", "tabs"]
}
